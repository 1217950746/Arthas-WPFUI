<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="PrimaryBrush" Color="#2FB2A9" />
    <SolidColorBrush x:Key="Background">#FFFFFF</SolidColorBrush>
    <SolidColorBrush x:Key="BorderBrush">#19000000</SolidColorBrush>
    <SolidColorBrush x:Key="Foreground">#3E3F48</SolidColorBrush>
    <SolidColorBrush x:Key="WindowBackground">#F5F7F9</SolidColorBrush>
    <LinearGradientBrush x:Key="CaptionBackground" StartPoint="0 1" EndPoint="1 0">
        <GradientStop Offset="0" Color="#36CCC2" />
        <GradientStop Offset="1" Color="#2FB2A9" />
    </LinearGradientBrush>
    <SolidColorBrush x:Key="CaptionForeground">#FFFFFF</SolidColorBrush>
</ResourceDictionary>